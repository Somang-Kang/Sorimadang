<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F2F8FF"
    tools:context=".HyunInstStudyIntro">

    <ImageView
        android:id="@+id/H_instBackimageView2"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:onClick="H_instBackActivity"
        app:layout_constraintBottom_toBottomOf="@+id/H_instTextView3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/H_instTextView3"
        app:srcCompat="@drawable/backward_arrow" />

    <TextView
        android:id="@+id/H_instTextView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/imhyeminbold"
        android:text="현악기"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/H_imageView"
        android:layout_width="70dp"
        android:layout_height="55dp"
        android:layout_marginTop="30dp"
        android:onClick="btnClickHyun"
        app:layout_constraintEnd_toEndOf="parent"
